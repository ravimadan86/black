.navbar-default .navbar-nav>li>a {
  color: #f0f3f5;
  font-size: 18px;
  padding: 15px 12px;
}
.navbar-default .navbar-nav>.open>a, .navbar-default .navbar-nav>.open>a:hover, .navbar-default .navbar-nav>.open>a:focus {
  background-color: #006699;
  color: #fff;
}
.navbar-default .navbar-nav>.active>a, .navbar-default .navbar-nav>.active>a:hover, .navbar-default .navbar-nav>.active>a:focus {
  color: #fff;
  background: none;
}
.navbar-default .navbar-nav>.active > a:after {
  background: url(../img/menu-selected.png) no-repeat center;
  display: block;
  width: 100%;
  height: 12px;
  content: "";
  position: absolute;
  z-index: 1;
  top: 100%;
  left: 0px;
}
.navbar-default .navbar-nav>li>a:hover, .navbar-default .navbar-nav>li>a:focus {
  color: #76cefa;
  background-color: transparent;
}
.navbar-default .navbar-nav>li>a .menuLabel{
  display: inline-block; color: #999999; 
}
.navbar-default .navbar-nav>li>a .userName{ color: #fff; font-size: 18px; }
.navbar-default .navbar-nav>li>a:hover .userName, .navbar-default .navbar-nav>li>a:focus
.userName{ color: #76cefa; }
