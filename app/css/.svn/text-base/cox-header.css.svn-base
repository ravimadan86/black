}
.header-primary {
  background-color: #333333;
  width: 100%;
}
.header-primary .logo{
  padding-top: 5px;
  display: block;
  float: left;
}

.btn {
  outline: none;
  padding: 7px 18px;
}
.btn.btn-link {
  color: #333;
  padding: 0px 5px;
}


