.custom-select {
  background-color: #fff;
  box-sizing: border-box;
  display: inline-block;
  width: 100%;
  vertical-align: top;
  height: 28px;
  padding: 0;
  position: relative;
  z-index: 1;
}
.custom-select.disabled{
  background-color: #eee;
}
.custom-select:before {
  content: url("../img/selectArrow2.png");
  display: block;
  height: 26px;
  position: absolute;
  right: 1px;
  text-align: center;
  top: 1px;
  width: 22px;
  z-index: -1;
}
.custom-select select.form-control[disabled]{
  cursor: not-allowed;
  background-color: transparent;
}
.custom-select select {
  background-color: transparent;
  border: 1px solid #b7b7b7;
  box-shadow: none;
  color: #333;
  display: block;
  font-size: 100%;
  height: 28px;
  line-height: 23px;
  padding: 0px 18px 1px 2px;
  padding: 0px 0px 0px 2px\0;/*IE 7 to 11 Hack*/
  margin: 0;
  width: 100%;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  -webkit-appearance: none;
  -moz-appearance: none;
  appearance: none;
}
:-moz-any(.custom-select):before {
    background-color: #fff;
    pointer-events: none;
    z-index: 1;
}
:-moz-any(.custom-select.disabled):before {
    background-color: #eee;
    pointer-events: none;
    z-index: 1;
}
@-moz-document url-prefix() { 
  .custom-select select{
      padding: 0px 0px 1px 2px;
  }
}

