.dateInputArea .btn.btn-sm{
  padding: 5px 10px;
}
.dateInputArea .dropdown-menu {
  background: #fff;
  width: 289px;
  visibility: visible;
  opacity: 1;
  filter: alpha(opacity=100);
}
.dateInputArea .dropdown-menu li > div {
