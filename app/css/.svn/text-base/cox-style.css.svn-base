}
.modal-footer {
  margin-top: 15px;
  padding: 10px 20px 10px!important;
  text-align: right;
  border-top: 1px solid #e5e5e5;
  background: #e4e5e9;
}
.btn-primary {
  color: #fff;
  background-color: #006599;
  border-color: #357ebd;
}
.btn-grey{
  color: #fff;
  background-color: #555;
  border-color: #444;
}
.btn-grey:hover, .btn-grey:focus, .btn-grey:active, .btn-grey.active{
  color: #fff;
  background-color: #666;
  border-color: #555;
  outline: none;
